{
  "name": "ComicBookConverter",
  "version": "1.0.0",
  "description": "Electron app to convert CBR (RAR4) files to CBZ format",
  "main": "src/main.js",
  "scripts": {
    "start": "npm run sass && electron .",
    "sass": "sass src/app.scss app.css --no-source-map --style=compressed",
    "watch:sass": "sass --watch src/app.scss:app.css --no-source-map"
  },
  "author": "Josh Chaiken contact@scatteredbrain.io",
  "license": "MIT",
  "devDependencies": {
    "electron": "^29.0.0",
    "sass": "^1.89.0"
  },
  "dependencies": {
    "archiver": "^6.0.0",
    "node-unrar-js": "^2.0.2"
  }
}
